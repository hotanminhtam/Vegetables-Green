        Scripts
    ***********************************-->
    <!-- Required vendors -->
    <script src="~/Assert/assert1/vendor/global/global.min.js"></script>
    <script src="~/Assert/assert1/js/quixnav-init.js"></script>
    <script src="~/Assert/assert1/js/custom.min.js"></script>



    <!-- Datatable -->
    <script src="~/Assert/assert1/vendor/datatables/js/jquery.dataTables.min.js"></script>
    <script src="~/Assert/assert1/js/plugins-init/datatables.init.js"></script>

</body>

</html>