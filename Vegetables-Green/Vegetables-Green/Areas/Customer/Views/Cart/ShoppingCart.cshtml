            <div class="container">
                <div class="banner-header banner-lbook3 space-30">
                    <img src="~/Assert/assert2/images/banner-blog.jpg" alt="Banner-header">
                    <div class="text">
                        <h3>Về chúng tôi</h3>
                        <p><a href="@Url.Action("Home", "Home")" title="Trang chủ">Trang chủ</a><i class="fa fa-caret-right"></i>Giỏ hàng</p>
                    </div>
                </div>
